<h1> index board threads </h1>
<div id="main-body">
  <% @board_threads.each do |board_thread|  %>
      <div id="threads-list">
        <%=
            link_to board_thread.title , board_thread
        %>
        created_at <%= board_thread.created_at.to_s %>
      </div>
  <% end  %>
  <%= link_to 'スレッド新規作成', :new_board_thread %>
</div>